{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build && cd ../server && npm install",
  "builds": [{"src": "./server/index.js", "use": "@vercel/node"}],
  "outputDirectory": "client/dist",
  "rewrites": [
     { "source": "/(.*)", "destination": "./server/index.js" }
  ]
}
